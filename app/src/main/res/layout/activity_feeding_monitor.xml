<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FeedingMonitorActivity">

    <TextView
        android:id="@+id/monitorLastDay"
        android:layout_width="152dp"
        android:layout_height="78dp"
        app:layout_constraintBottom_toTopOf="@+id/monitorLastWeek"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/monitorLastWeek"
        android:layout_width="152dp"
        android:layout_height="78dp"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toTopOf="@+id/monitorLastMonth"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monitorLastDay" />

    <TextView
        android:id="@+id/monitorLastMonth"
        android:layout_width="152dp"
        android:layout_height="78dp"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/monitorLastWeek" />
</androidx.constraintlayout.widget.ConstraintLayout>